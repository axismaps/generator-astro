{
  "typeAcquisition": {
      "include": [
          "jest"
      ]
  },
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@api/*": ["<%= src %>/api/*"],
      "@boot/*": ["<%= src %>/boot/*"],
      "@config/*": ["<%= src %>/config/*"],
      "@database/*": ["<%= src %>/database/*"],
      "@middlewares/*": ["<%= src %>/middlewares/*"],
      "@models/*": ["<%= src %>/models/*"],
      "@services/*": ["<%= src %>/services/*"],
      "@tests/*": ["<%= src %>/tests/*"],
      "@utils/*": ["<%= src %>/utils/*"],
      "@routes/*": ["<%= src %>/routes/*"],
      "@client/*": ["<%= client %>/*"],
      "@components/*": ["<%= client %>/components/*"],
      "@containers/*": ["<%= client %>/containers/*"],
      "@ducks/*": ["<%= client %>/ducks/*"],
      "@hox/*": ["<%= client %>/hox/*"]
    }
  }
}